import pandas as pd
import numpy as np

def compute_indicators(df):
    df = df.copy()

    df["MA60"] = df["Close"].rolling(60).mean()

    ema12 = df["Close"].ewm(span=12).mean()
    ema26 = df["Close"].ewm(span=26).mean()
    df["DIF"] = ema12 - ema26
    df["DEA"] = df["DIF"].ewm(span=15).mean()
    df["MACD"] = df["DIF"] - df["DEA"]

    return df
